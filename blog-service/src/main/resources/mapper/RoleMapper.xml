<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wrr.mapper.RoleMapper">

    <select id="getRoleList" resultType="com.wrr.dto.RoleListDTO">
        select
        a.role_name,
               count(a.role_name) as total
        from tb_role a
        group by a.role_name
    </select>
</mapper>
